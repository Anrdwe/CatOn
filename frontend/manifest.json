{
    "name": "Cat-On",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "cat"
    },
    "permissions": [
        "notifications","storage"
    ],
    "background": {
        "scripts": ["global.js", "background.js"]
    }
}